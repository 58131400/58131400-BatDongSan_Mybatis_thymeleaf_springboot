<?xml version = "1.0" encoding = "UTF-8"?>

<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

	<environments default="bds">
		<environment id="bds">
			<transactionManager type="JDBC" />

			<dataSource type="POOLED">
				<property name="driver" value="com.mysql.jdbc.Driver" />
				<property name="url" value="jdbc:mysql://localhost/batdongsandb" />
				<property name="username" value="root" />
				<property name="password" value="" />
			</dataSource>

		</environment>
	</environments>
<mappers>
<mapper class="com.example.thai.Mapper.UserMapper"/>
<mapper class="com.example.thai.Mapper.CamquyenMapper"/>
<mapper class="com.example.thai.Mapper.DistrictMapper"/>
<mapper class="com.example.thai.Mapper.DuanMapper"/>
<mapper class="com.example.thai.Mapper.GiaodichMapper"/>
<mapper class="com.example.thai.Mapper.LoaibdsMapper"/>
<mapper class="com.example.thai.Mapper.LoaigdMapper"/>
<mapper class="com.example.thai.Mapper.NhomnguoidungMapper"/>
<mapper class="com.example.thai.Mapper.NhomquyenMapper"/>
<mapper class="com.example.thai.Mapper.ProvinceMapper"/>
<mapper class="com.example.thai.Mapper.QuyenMapper"/>
<mapper class="com.example.thai.Mapper.ThemquyenMapper"/>
<mapper class="com.example.thai.Mapper.TinDangMapper"/>
<mapper class="com.example.thai.Mapper.TinhtrangphaplyMapper"/>
<mapper class="com.example.thai.Mapper.TintucMapper"/>

</mappers>

</configuration>